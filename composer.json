{
    "name": "{{toLowerCase vendor_name}}/{{toLowerCase report_name}}_report-extension",
    "type": "streams-addon",
    "description": "{{description}}",
    "keywords": [
        "streams-addon",
        "dashboard",
        "report",
        "laravel"
    ],
    "homepage": "{{homepage}}",
    "license": "{{license}}",
    "authors": [
        {
            "name": "{{author_name}}",
            "email": "{{email}}",
            "homepage": "{{homepage}}",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "{{email}}"
    },
    "autoload": {
        "psr-4": {
            "{{vendor_name}}\\\{{report_name}}ReportExtension\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
